{
  "name": "musicartistacademy",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others --prefix-colors \"red,green\" \"ng serve\" \"npm run windowsDebug\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "windowsDebug": "cd api & @powershell -Command $env:DEBUG='app,app:*';nodemon api/app.js",
    "postinstall": "cd api && npm install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
